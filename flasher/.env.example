# .env file for create_device.py
# Device prefix for auto-generated device IDs
DEVICE_PREFIX=auto

# The Things Network Application ID
TTN_APP_ID=your_app_id_here

# The Things Network API Key
TTN_API_KEY=your_api_key_here

# Climateguard API Key
CLIMATEGUARD_API_KEY=your_climateguard_api_key_here

# Application server address
APPLICATION_SERVER_ADDRESS=eu1.cloud.thethings.network

# Path to your PlatformIO project directory
PLATFORMIO_PROJECT_PATH=path\to\your\platformio\project  

# LoRaWAN version
LORAWAN_VERSION=1.0.2

# LoRaWAN PHY version
LORAWAN_PHY_VERSION=PHY_V1_0_2_REV_B

# Frequency plan ID
FREQUENCY_PLAN_ID=EU_863_870

# Whether the device supports join (true/false)
SUPPORTS_JOIN=true

# Baud rate for serial communication
BAUD_RATE=115200

# App key for LoRaWAN devices (16 bytes in hex format, with or without colons)
APP_KEY=00:00:00:00:00:00:00:00:00:00:00:00:00:00:00:00

# climateguard API url
CLIMATEGUARD_API_URL=https://api.quantum.hackerban.de/v2/